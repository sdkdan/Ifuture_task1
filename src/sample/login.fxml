<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="299.0" prefWidth="414.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <PasswordField fx:id="txtPassword" alignment="CENTER" layoutX="124.0" layoutY="165.0" promptText="Пароль" />
      <TextField fx:id="txtUsername" alignment="CENTER" layoutX="124.0" layoutY="115.0" promptText="Имя пользователя" />
      <Button fx:id="btnLogin" layoutX="178.0" layoutY="219.0" mnemonicParsing="false" onAction="#btnLoginAction" text="Войти" />
      <Button fx:id="btnReg" layoutX="157.0" layoutY="258.0" mnemonicParsing="false" onAction="#btnLoginReg" text="Регистрация" />
      <Label fx:id="lblMessage" layoutX="102.0" layoutY="54.0" text="Войдите или зарегистрируйтесь" />
   </children>
</AnchorPane>
